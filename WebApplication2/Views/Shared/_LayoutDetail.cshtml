<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700&display=swap" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.css" />
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick-theme.min.css" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
</head>
<body>
    <div class="header">
        <div class="container-box">
            <!-- logo & menu -->
            <div class="nav">
                <div class="logo">
                    <a class="nav-link" asp-area="" asp-controller="Product" asp-action="Index">
                        <img src="~/imagess/logo-one.png" alt="">
                    </a>
                </div>
                <div class="menu">
                    <ul class="menu-list">
                        <li class="menu-item"><a class="nav-link" asp-area="_Layout-Home" asp-controller="" asp-action="">Home</a></li>
                        <li class="menu-item"><a class="nav-link" href="#product">Products</a></li>
                        <li class="menu-item"><a class="nav-link" asp-area="" asp-controller="" asp-action="">About</a></li>
                        <li class="menu-item"><a class="nav-link" asp-area="" asp-controller="" asp-action="">Contact</a></li>

                    </ul>
                </div>
                <partial name="_LoginPartial" />
                <a href="@Url.Action("Index", "ShoppingCart")">
                    <img class="nav-cart" src="~/imagess/cart.png" alt="">
                </a>
            </div>
            <!-- end -->

        </div>
    </div>
    <div class="container mt-5 mb-5">
        <div class="row d-flex justify-content-center">
            <div class="col-md-10">
                <div class="card">
                    @RenderBody()
                    
                </div>
            </div>
        </div>

    </div>
   
    <!-- FOOTER -->
    <div class="footer">
        <div class="container">
            <div class="row">
                <!-- dowload app -->
                <div class="footer-col-1">
                    <h3>Download Our App</h3>
                    <p>Download App for Andriod and ios mobile phone.</p>
                    <div class="app-logo">
                        <img src="~/imagess/play-store.png" alt="">
                        <img src="~/imagess/app-store.png" alt="">
                    </div>
                </div>
                <!-- end -->
                <!-- logo footer -->
                <div class="footer-col-2">
                    <img src="~/imagess/logo-one.png" alt="">
                    <p>Our Purpose Is To Sustainably Make the Pleasure and Benefits of Sports Accessible to the Many.</p>

                </div>
                <!-- end -->
                <!-- useful link -->
                <div class="footer-col-3">
                    <h3>Userful Links</h3>
                    <ul>
                        <li>Coupons</li>
                        <li>Blog Post</li>
                        <li>Return Policy</li>
                        <li>Become Affiliate</li>
                    </ul>
                </div>
                <!-- end -->
                <!-- follow us -->
                <div class="footer-col-4">
                    <h3>Follow us</h3>
                    <ul>
                        <li>Facebook</li>
                        <li>Twitter</li>
                        <li>Instagram</li>
                        <li>Youtube</li>
                    </ul>
                </div>
                <!-- end -->

            </div>
            <!-- chan footer -->
            <hr>
            <p class="copyright">copyright 2021 - DinhNT Codes</p>
        </div>
    </div>
    <!-- END -->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>
